            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/MySymfony/web/app_dev.php/Home">My Website</a>
        </div>
        <div class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="nav-button-left"><a href="/MySymfony/web/app_dev.php/Home">Home</a></li>
            <li><a href="/MySymfony/web/app_dev.php/About">About</a></li>
            <li><a href="/MySymfony/web/app_dev.php/Resume">Resume</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>

{% endblock %}

{% block body %}
    <!-- Begin page content -->
  
{% endblock %}

{% block footer %}
