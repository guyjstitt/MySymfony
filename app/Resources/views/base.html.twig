        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}Bootstrap 101 Template{% endblock %}</title>
        {% block stylesheets %}


            <link href="{{ asset('bundles/framework/css/sticky-footer-navbar.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('bundles/framework/css/bootstrap/bootstrap.css') }}" type="text/css" rel="stylesheet" />
